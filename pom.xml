        <commons-beanutils>1.9.2</commons-beanutils>
        <org.apache.poi>3.10.1</org.apache.poi>
        <commons-io.version>2.4</commons-io.version>
        <commons-lang.version>2.6</commons-lang.version>
        <hibernate.version>4.3.8.Final</hibernate.version>
        <postgresql.version>9.3-1102-jdbc41</postgresql.version>
        <com.google.guava>18.0</com.google.guava>
        <org.atilika.kuromoji>0.7.7</org.atilika.kuromoji>
        <com.kawao.kakasi>0.4.0</com.kawao.kakasi>
