        </developer>
    </developers>

    <scm>
        <connection>scm:git:git://github.com/EsutoniaGoDesu/egd-web</connection>
        <url>scm:git:git://github.com/EsutoniaGoDesu/egd-web</url>
        <developerConnection>scm:git:git://github.com/EsutoniaGoDesu/egd-web</developerConnection>
