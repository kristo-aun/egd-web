    <name>EsutoniaGoDesu</name>
    <version>0.1-SNAPSHOT</version>

    <url>http://momo.koodur.com</url>

    <organization>
        <name>EsutoniaGoDesu</name>
        <url>https://twitter.com/EsutoniaGoDesu</url>
    </organization>

    <issueManagement>
        <url>https://github.com/EsutoniaGoDesu/egd-web/issues</url>
        <system>GitHub Issues</system>
    </issueManagement>

    <licenses>
        <license>
            <name>GPLv2</name>
            <url>http://www.gnu.org/licenses/gpl-2.0.html</url>
            <distribution>repo</distribution>
        </license>
    </licenses>

    <developers>
